# Pinecone
PINECONE_API_KEY=your_pinecone_api_key
PINECONE_ENVIRONMENT=gcp-starter   # or appropriate serverless region
PINECONE_INDEX_NAME=web-rag-index

# Groq
GROQ_API_KEY=your_groq_api_key